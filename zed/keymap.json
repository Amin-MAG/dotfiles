[
  {
    "bindings": {
      "cmd-shift-o": "workspace::Open",
      "cmd-o": "file_finder::Toggle",
      "cmd-p": "command_palette::Toggle",
      "ctrl-shift-i": "project_symbols::Toggle",
      "alt-cmd-o": "projects::OpenRecent",
      "alt-cmd-b": "branches::OpenRecent"
    }
  },
  {
    "context": "Editor && mode == full",
    "bindings": {
      "ctrl-i": "outline::Toggle"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "ctrl-shift-left": "pane::ActivatePrevItem",
      "ctrl-shift-tab": "pane::ActivatePrevItem",
      "ctrl-shift-h": "pane::ActivatePrevItem",
      "ctrl-shift-right": "pane::ActivateNextItem",
      "ctrl-tab": "pane::ActivateNextItem",
      "ctrl-shift-l": "pane::ActivateNextItem"
    }
  }
]
